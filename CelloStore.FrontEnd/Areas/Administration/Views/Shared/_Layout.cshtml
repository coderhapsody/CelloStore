<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Administration</title>
    @Html.Partial("~/Areas/Administration/Views/Shared/_References.cshtml")
</head>
<body>
    <div id="wrap">
        <header id="header">
            <nav class="navbar navbar-default navbar-fixed-top" role="navigation">
                <div class="container">
                    <div class="navbar-header">
                        <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
                            <span class="sr-only">Toggle navigation</span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                        </button>
                    </div>
                    <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                        @Html.Partial("~/Areas/Administration/Views/Shared/_LeftNav.cshtml")
                        @Html.Partial("~/Areas/Administration/Views/Shared/_RightNav.cshtml")
                    </div>
                </div>
            </nav>
        </header>
        <section>
            @RenderBody()
        </section>
    </div>
    <div id="footer">
        <div class="container">
            <p class="text-muted credit"><h5><small>Powered by <strong>CelloStore e-Commerce Content Management System - Version @ViewBag.VersionNumber</strong></small></h5>
            </p>
        </div>
    </div>
    @Scripts.Render("~/bundles/adminbootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
